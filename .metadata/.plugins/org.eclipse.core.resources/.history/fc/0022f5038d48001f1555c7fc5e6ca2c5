package kr.or.ddit.study07.phone;

public class PhoneMain {
	
	public static void main(String[] args) {
		HandPhone hp = new HandPhone();
		hp.call();
		hp.mobile();
		
		SmartPhone sp = new SmartPhone();
		sp.call();
		sp.mobile();
		sp.camera();
		sp.touch();
		
		iPhone ip = new iPhone();
		ip.appStore();
		ip.call();
		ip.camera();
		ip.mobile();
		ip.touch();
		
	}

}
