package kr.or.ddit.study06.sec02;

public class Student {
	String name;
	int kor;
	int eng;
	int math;
	
	int sum;
	double avg;

	// alt shift s   -> generate toString
	@Override
	public String toString() {
		return "학생 [name=" + name + ", kor=" + kor + ", eng=" + eng + ", math=" + math + ", sum=" + sum + ", avg="
				+ avg + "]";
	}
	
	

}
