package kr.or.ddit.study07;

public class ExtendObject extends SuperObject{
	
	public static void main(String[] args) {
		ExtendObject eo = new ExtendObject();
		eo.field1 = 10;
		eo.method1();
		System.out.println(eo.field1);
		
		
	}
	
	@Override
	public void method1() {
		// super 부모클래스를 의미함
		super.method1();
	}

}
