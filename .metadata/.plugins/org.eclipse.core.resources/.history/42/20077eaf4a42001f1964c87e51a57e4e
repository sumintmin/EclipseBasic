package kr.or.ddit.study06.sec03;

public class TvMain {
	public static void main(String[] args) {
		// 생성자를 만들어서 기본생성자가 안만들어진다.!!
//		Tv tv = new Tv();
		Tv tv = new Tv("벽걸이티비", "삼성", 100);
		// 요로케 값을 넣어주면 된다.
		
//		Tv t = new Tv("필드1", 2);
		
		System.out.println(tv.name + " " + tv.company + " " + tv.size);
	}
}
